{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://holochain.org/apps/mydata/publicdata.json",
  "type": "object",
  "required": ["fieldname", "value"],
  "properties": {
    "fieldname": {
      "$id": "/properties/fieldname",
      "type": "string",
      "pattern": ".+"
    },
    "value": {
      "$id": "/properties/value",
      "type": "object",
      "pattern": ".+"
    }
  }
}
